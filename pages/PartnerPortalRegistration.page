<script type="text/javascript">
            j$ = jQuery.noConflict();
            j$(document).ready(function() {
                j$('table[autotr]').each(function(index,val){
                    var count = parseInt(j$(val).attr('autotr'));
                    var cur = 0;
                    var tr = j$('<tr></tr>');
                    j$(val).find('td').each(function(i,v){
                        j$(tr).append(v);
                        cur++;
                        if(cur == count){
                            cur = 0;
                            j$(val).append(j$(tr));
                            tr = j$('<tr></tr>');
                        }
                    });
                    if(cur < count){
                        j$(val).append(j$(tr));
                    }
                });
                j$('table[autorequired]').each(function(index,val){
                    var required = j$(val).attr('autorequired').toLowerCase();
                    if(required == 'required' || required == 'true')
                    {
                    	markTableInputsAutoRequired(val);
                    	if(isTableUnchecked(val)){
                			toggleRequired(val,true);
                		}else{
                			toggleRequired(val,false);
                		}
                    	
                    }
                });
                j$('input[autoRequired]').change(function(){
                	var table = findRequiredTable(this);
                	if(j$(this).is(":checked")) {
                		toggleRequired(table, false);
                	}else if(isTableUnchecked(table)){
                		toggleRequired(table,true);
                	}
                });
            });
            function markPrimaryContact(cls, checked){
                if(checked){
                    j$("."+cls+".First_Name__c").val("{!hq.First_Name__c}");
                    j$("."+cls+".Last_Name__c").val("{!hq.Last_Name__c}");
                    j$("."+cls+".Address_1__c").val("{!hq.Address_1__c}");
                    j$("."+cls+".Address_2__c").val("{!hq.Address_2__c}");
                    j$("."+cls+".City__c").val("{!hq.City__c}");
                    j$("."+cls+".Region__c").val("{!hq.Region__c}");
                    j$("."+cls+".Region__c").trigger("change");
                    j$("."+cls+".Country__c").find("select").val("{!hq.Country__c}");
                    j$("."+cls+".Country__c").find("select").trigger("change");
                    j$("."+cls+".State__c").find("select").val("{!hq.State__c}");
                    j$("."+cls+".Postal_Code__c").val("{!hq.Postal_Code__c}");
                    j$("."+cls+".Email__c").val("{!hq.Email__c}");
                    j$("."+cls+".Phone__c").val("{!hq.Phone__c}");
                    j$("."+cls+".Fax__c").val("{!hq.Fax__c}");
                    
                }
                else{
                    j$("."+cls+".First_Name__c").val("");
                    j$("."+cls+".Last_Name__c").val("");
                    j$("."+cls+".Address_1__c").val("");
                    j$("."+cls+".Address_2__c").val("");
                    j$("."+cls+".City__c").val("");
                    j$("."+cls+".Region__c").val("");
                    j$("."+cls+".Country__c").val("");
                    j$("."+cls+".State__c").val("");
                    j$("."+cls+".Postal_Code__c").val("");
                    j$("."+cls+".Email__c").val("");
                    j$("."+cls+".Phone__c").val("");
                }
            }
            
            function toggleRequired(table, isRequired){
            	if(isRequired){
            		j$(table).find('input').each(function(i,v){
            			 j$(v).attr('required','required');
            		});            		
            	}
            	else{
            		j$(table).find('input').each(function(i,v){
            			j$(v).removeAttr('required');
            		});
            	}
            }
            
            function markTableInputsAutoRequired(table){
            	j$(table).find('input').each(function(i,v){
            			 j$(v).attr('autoRequired','required');
            		});
            }
            
            function findRequiredTable(input){
            	return j$(input).closest('table[autorequired]');
            }
            
            function isTableUnchecked(table){
            	var result = true;
            	j$(table).find('input').each(function(i){
            		if(j$(this).is(":checked")){
            			result = false;
            			return result;
            		}
            	});
            	return result;  
            }
            </script>
