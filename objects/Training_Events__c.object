    <webLinks>
        <fullName>MarkParticipantsAsSPXTrained</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Mark participants as SPX trained</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/12.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/12.0/apex.js&quot;)} 

var id = &apos;{!Training_Events__c.Id}&apos;;
var buttonValue = this.value; 

var changeButtonStatus = function(buttonValue, status, styleName) 
{ 
var buttons = document.getElementsByTagName(&apos;INPUT&apos;); 
for(var i = 0 ; i &lt; buttons.length ; i++) 
{ 
  if(buttons[i].value == buttonValue) 
   { 
    buttons[i].disabled = status; 
    buttons[i].className = styleName; 
   } 
} 
} 

var confirmInfo= confirm(&quot;Are you sure you want to mark all registered contacts for this class as having completed training? If not, remove any who did not attend first.&quot;);

if(confirmInfo == true)
{
     changeButtonStatus(buttonValue, true, &quot;btnDisabled&quot;); 
     sforce.apex.execute(&quot;CompleteSpxTrainingService&quot;,&quot;completeSpxTraining&quot;,{eventId:id},{
     onSuccess:function(result)  { 
           if(result &gt;1)
           {
              alert(result + &quot; registered contacts have been marked &quot;);
            }
            else
            {
               alert(result + &quot; registered contact has been marked &quot;);
            }
           changeButtonStatus(buttonValue, false, &quot;btn&quot;); 
          }, 
     onFailure:function(error)  { 
          alert(&quot;An error has occurred:&quot; + error);
          changeButtonStatus(buttonValue, false, &quot;btn&quot;); 
          } 
} );
}</url>
    </webLinks>
