    <webLinks>
        <fullName>Submit_for_Approval_Custom</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit for Approval</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/25.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/25.0/apex.js&quot;)}

if(confirm(&quot;Once you submit this record for approval, you might not be able to edit it or recall it from the approval process depending on your settings. Continue?&quot;))
{

var primaryQuote= sforce.connection.query(&quot;select Product_Type__c,Type from Opportunity where Id=&apos;{!Request__c.OpportunityId__c}&apos;&quot;);
var queueNameQueryResult = primaryQuote.getArray(&quot;records&quot;);

var quoteInfo= sforce.connection.query(&quot;Select Id from Quote__C where Primary__c=true and Opportunity__c =&apos;{!Request__c.OpportunityId__c}&apos; and Id in (Select Quote__c from Quote_Line__c)&quot;);
var quoteInfoQueryResult = quoteInfo.getArray(&quot;records&quot;);

if(queueNameQueryResult!=null &amp;&amp; queueNameQueryResult[0].Type!=&quot;Marketplace&quot; &amp;&amp; (quoteInfoQueryResult== null ||quoteInfoQueryResult==&apos;&apos;))
{
	alert(&quot;POC cannot be submitted for an approval as there is no primary quote defined for this opportunity or there are no quote lines added to the quote.&quot;);
}
else
{
	var approval = new sforce.ProcessSubmitRequest();
	approval.objectId = &quot;{!Request__c.Id}&quot;;
	var processRes = sforce.connection.process([approval]);
	window.location=(&quot;/{!Request__c.Id}&quot;);
}
}</url>
    </webLinks>
