    <validationRules>
        <fullName>Cant_submit_until_approved</fullName>
        <active>false</active>
        <description>Ensure that a request can&apos;t be moved out of Draft unless both approval flags are set</description>
        <errorConditionFormula>and( ISCHANGED(Status__c) , OR(not(RSM_Approved__c) , not(TR_Approved__c)),   ISPICKVAL( PRIORVALUE( Status__c), &quot;Draft&quot; ))</errorConditionFormula>
        <errorMessage>Both the RSM and Technically Responsible must review and approve this request before it can be submited for approvals</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ECSP_Poc_cant_be_created_for_non_servPr</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
AND
(
ISPICKVAL(Opportunity__r.Account.Partner_Type__c,&quot;Service Provider&quot;),
Opportunity__r.Account.ECSP__c ,
NOT(ISPICKVAL(POC_Type__c,&quot;Service Provider&quot; ))
),
AND
(
ISPICKVAL(Opportunity__r.Account.Partner_Type__c,&quot;Service Provider&quot;),
!Opportunity__r.Account.ECSP__c ,
ISPICKVAL(POC_Type__c,&quot;Service Provider&quot; )
),
AND(
NOT(ISPICKVAL(Opportunity__r.Account.Partner_Type__c,&quot;Service Provider&quot;)),
ISPICKVAL( POC_Type__c,&quot;Service Provider&quot; )
)
)</errorConditionFormula>
        <errorDisplayField>POC_Type__c</errorDisplayField>
        <errorMessage>The selected POC Type is not valid for this account. Service Provider is valid for ECSP Enabled Service Provider accounts . Edge Connect and Wan OP is valid for non-Service Provider accounts.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_Open_as_Draft</fullName>
        <active>true</active>
        <description>doesn&apos;t allow status to be changed from default of draft</description>
        <errorConditionFormula>AND(  ISNEW() , not(ISPICKVAL( PRIORVALUE( Status__c), &quot;Open&quot; )),  Marketplace_Sourced_Opp__c =0,  IsAllVirtual__c =0)</errorConditionFormula>
        <errorMessage>Request can only be opened in stage &quot;Open&quot;. After RSM and TR approvals are made it can be submitted for approval</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>POC_cant_be_created_without_prim_quotes</fullName>
        <active>true</active>
        <description>A POC cannot be created if there is no primary quote.</description>
        <errorConditionFormula>AND( 
IsNew(),
NOT(ISBLANK(Opportunity__c)),
NOT(ISPICKVAL(Opportunity__r.Type ,&quot;Marketplace&quot;)), 
ISPICKVAL(Opportunity__r.Product_Type__c,&quot;&quot;))</errorConditionFormula>
        <errorMessage>POC cannot be created for the selected opportunity as there is no primary quote defined.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RSM_of_Oppty_Approves</fullName>
        <active>true</active>
        <description>Ensure that only the oppty owner can approve</description>
        <errorConditionFormula>and($User.Id &lt;&gt; Opportunity__r.OwnerId , ISCHANGED( RSM_Approved__c ), RSM_Approved__c =True)</errorConditionFormula>
        <errorMessage>Only the oportunity owner can approve this request</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Req_End_Date_after_Target_End_Date</fullName>
        <active>true</active>
        <description>The Request End Date must always be after the Target End Date.</description>
        <errorConditionFormula>IF (AND($Profile.Name &lt;&gt; &apos;9-Operations Manager&apos;), 
AND( 
OR ( 
ISNEW(), 
ISCHANGED( Requested_End_Date__c )), 
Requested_End_Date__c &lt; Target_End_Date__c || Requested_End_Date__c = Target_End_Date__c 
) 
, 
false)</errorConditionFormula>
        <errorMessage>Your Requested End Date must be after the Target End Date of the POC.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Req_End_Date_notreq_OnInsert</fullName>
        <active>true</active>
        <description>The Request End Date is not required when the POC is created for the first time</description>
        <errorConditionFormula>IF (AND($Profile.Name &lt;&gt; &apos;9-Operations Manager&apos;), 
AND( ISNEW(),!ISNULL(Requested_End_Date__c )) 
, 
false)</errorConditionFormula>
        <errorMessage>Your Requested End Date can be entered for POC extensions only.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Requested_End_Date_cannot_exceed_30_days</fullName>
        <active>true</active>
        <errorConditionFormula>OR( 
ISPICKVAL( POC_Type__c ,&quot;WAN Op&quot;), 
ISPICKVAL( POC_Type__c ,&quot;EdgeConnect&quot;) 
)&amp;&amp; 
NOT(ISBLANK( Requested_End_Date__c )) &amp;&amp; 
NOT(ISBLANK( Target_End_Date__c )) &amp;&amp; 
ISCHANGED(Requested_End_Date__c) &amp;&amp; 
Requested_End_Date__c - Target_End_Date__c &gt; 30</errorConditionFormula>
        <errorMessage>Requested End Date cannot be more than 30 days.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Requested_End_Date_cannot_exceed_90_days</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( POC_Type__c ,&quot;Service Provider&quot;)&amp;&amp; 
NOT(ISBLANK( Requested_End_Date__c )) &amp;&amp; 
NOT(ISBLANK( Target_End_Date__c )) &amp;&amp; 
ISCHANGED(Requested_End_Date__c) &amp;&amp; 
Requested_End_Date__c - Target_End_Date__c &gt; 90</errorConditionFormula>
        <errorMessage>Requested End Date cannot be more than 90 days.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TR_Approval_Check</fullName>
        <active>true</active>
        <description>Validate that only the TR can check the approval box</description>
        <errorConditionFormula>and($User.Id &lt;&gt;  Opportunity__r.Technical_Responsible__r.Id , ISCHANGED( TR_Approved__c ), TR_Approved__c =true)</errorConditionFormula>
        <errorMessage>Only the person selected as Technical Responsibility can approve this request</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>enforce_approval_process_to_advance</fullName>
        <active>true</active>
        <description>Enforce new approval submission instead of changing status</description>
        <errorConditionFormula>and( ISCHANGED(Status__c) ,  
OR(ISPICKVAL(Status__c, &quot;Open&quot; ),
ISPICKVAL(Status__c, &quot;Approved&quot; ),
ISPICKVAL(Status__c, &quot;Approved to Ship&quot; ),
ISPICKVAL(Status__c, &quot;Shipped&quot; ),
ISPICKVAL(Status__c, &quot;Shipped - Extended&quot; ),
ISPICKVAL(Status__c, &quot;Pending Return - Invoiced&quot; ),
ISPICKVAL(Status__c, &quot;Closed - Won&quot; ),
ISPICKVAL(Status__c, &quot;Closed - Denied&quot; ),
ISPICKVAL(Status__c, &quot;Closed - Returned&quot; ),
ISPICKVAL(Status__c, &quot;Scheduled&quot; )),
OR($User.Id = Opportunity__r.OwnerId, $User.Id = Opportunity__r.Technical_Responsible__r.Id),  Marketplace_Sourced_Opp__c =0
)</errorConditionFormula>
        <errorMessage>Use the Submit for Approval button instead of changing the status to request approval</errorMessage>
    </validationRules>
