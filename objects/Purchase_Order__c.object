    <webLinks>
        <fullName>Mark_Shipped</fullName>
        <availability>online</availability>
        <description>used for manually marking shipped when no shipment record is processed</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Mark Shipped</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/13.0/connection.js&quot;)} 

var poObj = new sforce.SObject(&quot;Purchase_Order__c&quot;); 
poObj.Id = &apos;{!Purchase_Order__c.Id}&apos;; 
poObj.Status__c = &apos;Shipped - Complete&apos;; 
var result = sforce.connection.update([poObj]); 

if (result[0].success==&apos;false&apos;) { 
alert(result[0].errors.message); 
} else { 
location.reload(true); 
}</url>
    </webLinks>
